{
    "name": "jskrd/laravel-shop",
    "description": "A package for Laravel based projects providing a shop web API.",
    "keywords": ["laravel", "shop"],
    "license": "MIT",
    "require": {
        "php": "^7.3",
        "league/iso3166": "^2.1",
        "stripe/stripe-php": "^7.52"
    },
    "require-dev": {
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "3.*"
    },
    "autoload": {
        "psr-4": {
            "Jskrd\\Shop\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jskrd\\Shop\\ShopServiceProvider"
            ]
        }
    }
}
